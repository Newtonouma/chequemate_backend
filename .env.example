# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=development
PORT=3002

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/your_database

# ONIT Payment API Configuration
ONIT_CLIENT_ID=your_client_id_here
ONIT_CLIENT_SECRET=your_client_secret_here
ONIT_MERCHANT_NUMBER=your_merchant_number_here

# Callback URLs
# For local development with ngrok:
# ONIT_CALLBACK_URL=https://your-ngrok-url.ngrok-free.dev/api/payments/callback
# For production (Render):
# ONIT_CALLBACK_URL=https://chequemate-backend-n13g.onrender.com/api/payments/callback

# Chess.com API Configuration
CHESS_COM_USERNAME=your_chess_username
CHESS_COM_EMAIL=your_email@example.com

# JWT Secret
JWT_SECRET=your_super_secret_jwt_key_here_change_this_in_production
